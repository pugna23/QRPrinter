@model WebApplication1.Models.PrintsViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>

@*<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title></title>
    </head>
    <body>*@

<div class="a4">
    @foreach (var item in Model.Prints.Select((value, i) => new { i, value }))
    {

        <div class="contenedorQR">
            <span>@item.value.Title</span>
            @{
                var base64 = Convert.ToBase64String(item.value.Img);
                var imgSrc = String.Format("data:image/gif;base64,{0}", base64);
            }
            <img class="qr" src="@imgSrc" />
        </div>
    }
</div>

<button onClick="window.print()"type="button" class="btn btn-success fixed-button hidden-print">Imprimir</button>

